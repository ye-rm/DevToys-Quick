{
  "package": "com.devtoys.quickapp",
  "name": "DevToys-Quick",
  "versionName": "1.0.2",
  "versionCode": 1,
  "minPlatformVersion": 1080,
  "icon": "/assets/images/logo.png",
  "features": [
    { "name": "system.clipboard" },
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.media" },
    { "name": "system.file" },
    { "name": "system.device"},
    { "name": "system.battery"},
    { "name": "system.network"},
    { "name": "service.asr"},
    { "name": "system.vibrator"}
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {},
  "router": {
    "entry": "pages/home/index",
    "pages": {
      "pages/home/index": {
        "component": "index"
      },
      "pages/base64": {
        "component": "index"
      },
      "pages/json-formatter": {
        "component": "index"
      },
      "pages/xml-formatter": {
        "component": "index"
      },
      "pages/password-gen": {
        "component": "index"
      },
      "pages/number-base": {
        "component": "index"
      },
      "pages/hash-gen": {
        "component": "index"
      },
      "pages/checksum": {
        "component": "index"
      },
      "pages/speech2text":{
        "component": "index"
      },
      "pages/system-info": {
        "component": "index"
      },
      "pages/about": {
        "component": "index"
      }
    }
  },
  "display": {
    "themeMode": 0,
    "backgroundColor": "#ffffff",
    "titleBarBackgroundColor": "#ffffff",
    "titleBarTextColor": "#000000"
  }
}